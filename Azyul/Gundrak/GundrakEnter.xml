<If Condition="Me.MapId != 604 &amp;&amp; Me.FreeBagSlots &gt;= Settings.Instance.VendorMinBagSlots" >
	<If Condition="Me.Location.Distance(new WoWPoint(6944.809, -4426.977, 450.8831)) &gt; 300 " >
		<CustomBehavior File="RunCode" Code="Logging.Write(System.Windows.Media.Colors.DeepSkyBlue, &quot;[Azyul]: Heading to Instance.&quot;);" />
	</If>

	<CustomBehavior File="RunCode" Code="Logging.Write(System.Windows.Media.Colors.DeepSkyBlue, &quot;Moving to position 1 of dungeon&quot;);" />
	
	<If Condition="CanFly()" >
		<CustomBehavior File="FlyTo" X="6968.047" Y="-4408.18" Z="457.2327" />
		<Else>
			<MoveTo Nav="Fly" X="6968.047" Y="-4408.18" Z="457.2327" />
		</Else>
</If>
	

		<If Condition="(Me.HasAura(5215))">
			
			<CustomBehavior File="RunCode" Code="Logging.Write(System.Windows.Media.Colors.DeepSkyBlue, &quot;Entering Dungeon while stealthed&quot;);" />
			<CustomBehavior File="MyCTM" X="6975.58" Y="-4400.046" Z="441.5751" TerminateWhen="Me.IsInInstance" />
		<!--	<CustomBehavior File="GoThruPortal" Timeout="30000" X="6975.58" Y="-4404.028" Z="442.5896" /> -->

		<Else>
 			<CustomBehavior File="RunCode" Code="Logging.Write(System.Windows.Media.Colors.DeepSkyBlue, &quot;We don't have stealth, moving near entrance&quot;);" />
			<CustomBehavior File="RunCode" Code="Logging.Write(System.Windows.Media.Colors.DeepSkyBlue, &quot;Cancelling fly form&quot;);" />
		<!--		<CustomBehavior File="RunMacro" Macro="/cancelform" />  -->
			<CustomBehavior File="MyCTM" X="6975.58" Y="-4400.046" Z="441.5751" TerminateWhen="Me.IsInInstance" />
			<!--	<CustomBehavior File="GoThruPortal" Timeout="30000" X="6975.58" Y="-4404.028" Z="442.5896" />  -->
 	
   
		</Else>
		</If>





	<CustomBehavior File="WaitTimer" WaitTime="10000" TerminateWhen="Me.IsInInstance" />
</If>



